{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "jsx": "react-jsx",
    "module": "ESNext",
    "rootDir": "./src",
    "checkJs": true,
    "inlineSourceMap": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "typeRoots": [],
    "types": []
  },
  "include": [
    "src"
  ],
  "exclude": [
    "**/ * .d.ts",
    "dist",
    "node_modules"
  ]
}
